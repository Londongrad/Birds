<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:conv="clr-namespace:Birds.UI.Converters">

    <conv:DateOnlyToDateTimeConverter x:Key="DateOnlyConverter" />
    <conv:InverseBoolConverter x:Key="BoolConverter" />
    <conv:MultiBoolToVisibilityConverter x:Key="MultiBoolToVisibilityConverter" />
    <conv:BoolToVisibilityInverseConverter x:Key="BoolToVisibilityInverse" />
    <conv:BoolToStatusTextConverter x:Key="BoolToStatusTextConverter" />
    <conv:BoolToColorConverter x:Key="BoolToColorConverter" />
    <conv:BirdVmConverter
        x:Key="BirdVmConverter"
        Factory="{x:Null}" />
    <BooleanToVisibilityConverter x:Key="BoolToVisibilityConverter" />
</ResourceDictionary>